#!/usr/bin/env python3
from modules.apps.cli import App
from modules.config.cli import Config
from modules.compose.cli import Compose
from modules.env.cli import Env
import fire


class Composer(object):
    def __init__(self):
        self.app = App
        self.config = Config
        self.compose = Compose
        self.env = Env

    def __str__(self):
        return 'Composer CLI'


if __name__ == '__main__':
    fire.Fire(Composer)
